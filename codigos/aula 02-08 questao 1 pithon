def exibir_numeros_inversos(numeros: list) -> None:
    """
    Função que exibe uma lista de números na ordem inversa de inserção.
    Parâmetros:
    numeros (list): Lista de números inteiros inseridos pelo usuário.
    Retorna:
    None
    """
    for numero in reversed(numeros):
        print(numero)

def main():
    numeros = []
    n = int(input("Quantos números você deseja inserir? "))
    for _ in range(n):
        numero = int(input("Digite um número: "))
        numeros.append(numero)
    
    print("Números na ordem inversa:")
    exibir_numeros_inversos(numeros)

if __name__ == "__main__":
    main()
